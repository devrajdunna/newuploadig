<p id="notice"><%= notice %></p>

<h1>Crkpurams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crkpurams.each do |crkpuram| %>
      <tr>
        <td><%= link_to 'Show', crkpuram %></td>
        <td><%= link_to 'Edit', edit_crkpuram_path(crkpuram) %></td>
        <td><%= link_to 'Destroy', crkpuram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crkpuram', new_crkpuram_path %>
