<p id="notice"><%= notice %></p>

<h1>Crkpuramdata</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Care of</th>
      <th>Adhar</th>
      <th>Mobile</th>
      <th>Bank name</th>
      <th>Ifsccode</th>
      <th>Account number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crkpuramdata.each do |crkpuramdatum| %>
      <tr>
        <td><%= crkpuramdatum.full_name %></td>
        <td><%= crkpuramdatum.care_of %></td>
        <td><%= crkpuramdatum.adhar %></td>
        <td><%= crkpuramdatum.mobile %></td>
        <td><%= crkpuramdatum.bank_name %></td>
        <td><%= crkpuramdatum.ifsccode %></td>
        <td><%= crkpuramdatum.account_number %></td>
        <td><%= link_to 'Show', crkpuramdatum %></td>
        <td><%= link_to 'Edit', edit_crkpuramdatum_path(crkpuramdatum) %></td>
        <td><%= link_to 'Destroy', crkpuramdatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crkpuramdatum', new_crkpuramdatum_path %>
